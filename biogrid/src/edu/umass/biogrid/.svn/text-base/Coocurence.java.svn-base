package edu.umass.biogrid;

import java.util.List;

/**
 * @author qing
 *
 */
public class Coocurence {
	protected int cooccur;
	protected int union;
	protected int src;
	protected int dest;
	protected List<String> srcPMIDs;
	protected List<String> destPMIDs;
	
	public String toString(){
		StringBuffer sb = new StringBuffer();
		sb.append("cooccur:").append(cooccur).append('\n');
		sb.append("union:").append(union).append('\n');
		sb.append("src:").append(src).append('\n');
		sb.append("dest:").append(dest).append('\n');
		sb.append("srcPMIDs:").append(srcPMIDs.size()).append('\n');
		sb.append("destPMIDs:").append(destPMIDs.size()).append('\n');
		return sb.toString();
	}

}
